@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>

@{
    ViewBag.Title = "Roles";
}

    <h2>Roles</h2>

<p>
    @Html.ActionLink("Create Role", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Role Name
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
    }
</table>
