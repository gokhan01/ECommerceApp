@model List<EcommerceApp.DAL.Concrete.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>

<p>
    @Html.ActionLink("Yetkilendirme", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Kullanıcılar
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Yetkilendir", "Create", new { userId = item.Id })
                @*@Html.ActionLink("Details", "Details", new { id = item.Id }) |*@
                @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
            </td>
        </tr>
    }

</table>
